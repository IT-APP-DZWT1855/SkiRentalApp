@model List<SkiRental.Models.Item>

@if (Model.Count == 0)
{
    <div class="alert alert-warning">@ViewBag.orderCode</div>
}

<table>
    <tr>
        <th>Id</th>
        <th>Nazwa</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Model</td>
        </tr>
    }
</table>